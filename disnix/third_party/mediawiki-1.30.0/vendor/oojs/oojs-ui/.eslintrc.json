{
    "extends": "wikimedia",
    "parserOptions": {
        "ecmaVersion": 5
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "qunit": true
    },
    "globals": {
        "OO": false
    },
    "rules": {
        "dot-notation": [ "error", { "allowKeywords": true } ],
        "no-void": 0,
        "valid-jsdoc": 0
    }
}
